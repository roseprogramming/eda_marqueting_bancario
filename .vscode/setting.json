{
  "python.analysis.typeCheckingMode": "off",
  "python.analysis.extraPaths": [
    "${workspaceFolder}",
    "${workspaceFolder}/src"
  ],
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/Scripts/python.exe",
  "jupyter.notebookFileRoot": "${workspaceFolder}",
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.pyc": true
  }
}
